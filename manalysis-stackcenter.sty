			\exp_args:NNe \clist_put_right:Nn #1 { \clist_item:Nn #1 { \clist_count:N #1 } }
			\exp_args:NNe \clist_put_right:Nn #1 { \clist_item:Nn #1 { \clist_count:N #1 } }
		}
}

