{A module for regex powered frac command}

\cs_new:Nn \__manalysis_define_letters:Nn {
    \clist_map_inline:nn {#2} {
        #1 {##1}
    }
}

\cs_new:Nn \__manalysis_define_mathbb_letter:n {
    \expandafter\let \csname #1#1\endcsname \relax
    \cs_new:cpn {#1#1} {\mathbb{#1}}
}

\cs_new:Nn \__manalysis_define_mathbf_letter:n {
    \expandafter\let \csname #1bf\endcsname \relax
    \expandafter\let \csname bf#1\endcsname \relax
    \cs_new:cpn {#1bf} {\mathbf{#1}}
    \cs_new:cpn {bf#1} {\mathbf{#1}}
}

\cs_new:Nn \__manalysis_define_mathfrak_letter:n {
    \expandafter\let \csname #1frak\endcsname \relax
    \expandafter\let \csname frak#1\endcsname \relax
    \cs_new:cpn {#1frak} {\mathfrak{#1}}
    \cs_new:cpn {frak#1} {\mathfrak{#1}}
}

\cs_new:Nn \__manalysis_define_mathrm_letter:n {
    \expandafter\let \csname #1rm\endcsname \relax
    \expandafter\let \csname rm#1\endcsname \relax
    \cs_new:cpn {#1rm} {\mathrm{#1}}
    \cs_new:cpn {rm#1} {\mathrm{#1}}
}

